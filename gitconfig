[github]
    user = kristenmills
[user]
    name = Kristen Mills
    email = kristen@found.com
	signingkey = 91761CEF79A0B251
[color]
    ui = auto
  [color "branch"]
    current = yellow reverse
    local = yellow
    remote = green
  [color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red bold
    new = green bold
  [color "status"]
    added = yellow
    changed = green
    untracked = cyan

[alias]
    uncommit = reset --soft HEAD^
    unstage = reset
    unstash = stash pop
    st = status
    ci = commit
    br = branch
    co = checkout
    df = diff
    dc = diff --cached
    lg = log -p
    lol = log --graph --decorate --pretty=oneline --abbrev-commit
    lola = log --graph --decorate --pretty=oneline --abbrev-commit --all
    ls = ls-files
    nfmerge = merge --no-ff
[push]
    default = simple
	autoSetupRemote = true
[help]
    autocorrect = 1
[credential]
    helper = osxkeychain
[core]
    excludesfile = ~/.gitignore_global
[filter "media"]
    clean = git media clean %f
    smudge = git media smudge %f
    required = true
[filter "lfs"]
    clean = git-lfs clean %f
    smudge = git-lfs smudge %f
    required = true
[url "git@github.com:"]
  insteadOf = https://github.com/
[pull]
	rebase = true
[commit]
	gpgsign = false
