{
  "segments": {
    "right": [
      {
        "function": "segments.spotify.spotify",
        "name": "player"
      },
      {
        "function": "powerline.segments.common.wthr.weather",
        "args": {
          "unit": "F",
          "icons": {
            "day":           "🌤",
            "blustery":      "💨",
            "rainy":         "🌧️ ",
            "cloudy":        "☁",
            "snowy":         "❅",
            "stormy":        "⛈",
            "foggy":         "🌫",
            "sunny":         "☀️",
            "night":         "☾",
            "windy":         "💨",
            "not_available": "�",
            "unknown":       "⚠"
          }
        }
      },
      {
        "function": "powerline.segments.common.time.date",
        "name": "time",
        "args": {
          "format": "%-I:%M %p",
          "istime": true
        }
      },
      {
        "function": "powerline.segments.common.time.date"
      },
      {
        "function": "powerline.segments.common.bat.battery"
      },
      {
        "function": "powerline.segments.common.net.hostname"
      }
    ]
  }
}
