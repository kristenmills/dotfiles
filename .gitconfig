[github]
	user = kristenmills
[user]
	name = kristenmills
	email = kristen@kristen-mills.com
[color]
    ui = auto
  [color "branch"]
    current = yellow reverse
    local = yellow
    remote = green
  [color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red bold
    new = green bold
  [color "status"]
    added = yellow
    changed = green
    untracked = cyan

[alias]
    uncommit = reset --soft HEAD^
	unstage = reset
	unstash = stash pop
	st = status
    ci = commit
    br = branch
    co = checkout
    df = diff
    dc = diff --cached
    lg = log -p
    lol = log --graph --decorate --pretty=oneline --abbrev-commit
    lola = log --graph --decorate --pretty=oneline --abbrev-commit --all
    ls = ls-files
	nfmerge = merge --no-ff
[push]
	default = simple
[core]
	excludesfile = /Users/kristen/.gitignore_global
